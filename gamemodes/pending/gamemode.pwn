			GetVehicleHealth(tmpcarx, vehHealth);
			if(vehHealth > 800)return SendClientMessage(playerid,COLOR_RED,"[Info]: {FFFFFF}Das Fahrzeug muss nicht repariert werden (über 800 HP).");
