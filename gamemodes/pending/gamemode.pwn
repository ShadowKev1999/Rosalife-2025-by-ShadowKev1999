    SpielerInfo[playerid][sLotto][0] = Lotto[0];SpielerInfo[playerid][sLotto][1] = Lotto[1];SpielerInfo[playerid][sLotto][2] = Lotto[2];
	SendClientMessage(playerid,COLOR_RED,"[Info]: {FFFFFF} Du hast einen Lottoschein gefunden.");
