		if(GetPVarInt(playerid,"GeldkofferInHand") == 0)return SendClientMessage(playerid,COLOR_RED,"[Info]: {FFFFFF}Benutze '/geldkoffer' um, das Geld hinten aus dem Geldtransporter herraus zu holen.");
