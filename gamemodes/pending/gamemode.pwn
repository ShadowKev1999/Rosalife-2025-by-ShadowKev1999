    	if(SpielerInfo[playerid][sFraktion] == 2 || SpielerInfo[playerid][sFraktion] == 6)return SendClientMessage(playerid,COLOR_RED,"[Info]: {FFFFFF}Du hast eine Schlüssel-Karte. Benutze die 'N' Taste.");
