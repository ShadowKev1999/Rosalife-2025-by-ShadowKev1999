			new merker = 0;
			foreach(new i : Player)
			{
				if(SpielerInfo[i][sHandy] == Nummer)
				{
				    merker = i;
				}
			}
			if(merker < 1)return SendClientMessage(playerid,COLOR_GREY,"[Info]: HGPS-ORTUNG: Das Handy konnte nicht ermittelt werden (StatusC=404NotFound)");
