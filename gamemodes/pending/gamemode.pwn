			    WaitToFindTimer[playerid] = SetTimerEx("WaitToFindDetektiv", 30000, false, "i", playerid);
			}
			new Float:gpsOrtPos[3];
			GetPlayerPos(merker,gpsOrtPos[0],gpsOrtPos[1],gpsOrtPos[2]);
