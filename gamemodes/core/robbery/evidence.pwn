forward PickupTimer();
public PickupTimer()
{
	if(LSAsservatenkammerDefuseTime > 0){
	    if(IsPlayerInRangeOfPoint(LSAsservatenkammerDefuser, 2,1821.2428,-1941.9307,1592.9390)){
	        LSAsservatenkammerDefuseTime -= 1;
	        if(LSAsservatenkammerDefuseTime == 10){GameTextForPlayer(LSAsservatenkammerDefuser,"~b~Noch ~g~10 ~b~Sekunden", 3000, 3);}
	        if(LSAsservatenkammerDefuseTime == 0){
         		if(LSAsservatenkammerTimer < 6){
		           	new sendername[MAX_PLAYER_NAME], string[256];
				   	GetPlayerName(LSAsservatenkammerDefuser,sendername,sizeof(sendername));
        			format(string, sizeof(string), "HQ: %s hat das Dynamit an der Asservatenkammer Los Santos erfolgreich entschärft.",sendername);
           			SendCopMessage(COLOR_BLUE,string);
					{
					   SendClientMessageToAll(COLOR_NEWS,"San News: Achtung, es wird aktuell gemeldet, dass das SAPD die Überhand über");
					   SendClientMessageToAll(COLOR_NEWS,"San News: die Situation an der Asservatenkammer in Los Santos behalten hat und diesen entschärft hat.");
					   SendClientMessageToAll(COLOR_NEWS,"San News: Bleiben Sie dran für weitere Meldungen!");
					}
					LSAsservatenkammerDefuseTime = 0;
					LSAsservatenkammerDefuser = -1;
					LSAsservatenkammerTimer = 10;
					MoveObject(AsservatenkammerBombe, 307.12, -1489.05, 20.50, 10);
					//TresorrobStatus = 0;
				}
				else
				{
				    SendClientMessage(LSAsservatenkammerDefuser,COLOR_ERRORTEXT,"Entschärfen des Dynamit's Fehlgeschlagen !");
				    LSAsservatenkammerDefuser= -1;
				    LSAsservatenkammerDefuseTime = 0;
				}
			}
		}
		else
		{
  			SendClientMessage(LSAsservatenkammerDefuser,COLOR_ERRORTEXT,"Du bist nicht mehr an der Asservatenkammer um das Dynamit zu entschärfen!");
   			LSAsservatenkammerDefuser= -1;
		    LSAsservatenkammerDefuseTime = 0;
		}
	}
	return 1;
}

forward MinutenTimer();
public MinutenTimer()
{
	if(LSAsservatenkammerSperre > 0)
	{
	    LSAsservatenkammerSperre -= 1;
	}
	if(LSAsservatenkammerTimer < 10 || LSAsservatenkammerTimer == 19 || LSAsservatenkammerTimer == 20){

		if(LSAsservatenkammerTimer <= 5 && LSAsservatenkammerTimer >= 1)
		{
		    new msg[128];
		    format(msg, sizeof(msg), "[Asservatenkammer] Noch %d Minute(n) bis das Dynamit in Los Santos explodiert.", LSAsservatenkammerTimer);
		    SendClientMessageToAll(COLOR_YELLOW, msg);
		    LSAsservatenkammerTimer -= 1;
		}
		else if(LSAsservatenkammerTimer == 0){
		    SendClientMessageToAll(COLOR_YELLOW,"[Asservatenkammer] Das Dynamit in Los Santos ist explodiert.");
		    //AsservatenkammerrobStatus = 0;
		    LSAsservatenkammerTimer = 20;
		    MoveObject(AsservatenkammerBombe, 307.12, -1489.05, 25.50, 10);
		    MoveObject(LSAsservatenkammerTor, 1821.5190, -1940.8958, 1580.2070, 10);
		    CreateExplosion(1821.5190, -1940.8958, 1593.2070, 6, 5.0);
		    LSAsservatenkammerPickupAllow = 1;
		}
		else if(LSAsservatenkammerTimer == 20){
		    LSAsservatenkammerTimer = 19;
		}
		else if(LSAsservatenkammerTimer == 19){
		    MoveObject(LSAsservatenkammerTor, 1821.5190, -1940.8958, 1593.2070, 10);
		    LSAsservatenkammerTimer = 10;
	}}
	return 1;
}

stock Evidence_OnPlayerPickUpPickup(playerid, pickupid) {
    if(pickupid == LSAsservatenkammerPickup)
	{
 		if(LSAsservatenkammerPickupAllow == 1)
	    {
		    new sendername[MAX_PLAYER_NAME], string3[256], fID;
		    GetPlayerName(playerid,sendername,sizeof(sendername));
			if(SpielerInfo[playerid][sFraktion] != 1)
			{
			  	format(string3, sizeof(string3), "HQ: %s hat die Asservatenkammer in Los Santos erfolgreich ausgeraubt.",sendername);
			  	SendCopMessage(COLOR_BLUE,string3);
				{
				   new string31[1000];
				   format(string31, sizeof(string31), "ACHTUNG: %s hat die Asservatenkammer in Los Santos erfolgreich ausgeraubt!",sendername);
				   SendClientMessageToAll(COLOR_YELLOW, string31);
				}
				new drugs = 150+ random(50);
				SpielerInfo[playerid][sDrogen] += drugs;
				new ak1 = 7500+ random(5000);
				FrakInfo[fID][fiMats] += ak1;
				//factionInfo[factionId][Waffenpaket1] += ak1;
				new ak2 = 10000+ random(5000);
				new ak3 = 10000+ random(5000);
				FrakInfo[fID][fiFrakKasse] += ak2;
				SpielerInfo[playerid][sBankkonto] += ak3;

				LSAsservatenkammerPickupAllow = 0;
				format(string3,sizeof(string3),"Durch das ausrauben der Asservatenkammer, erhälst du %d Drogen und %d$.",drugs,ak3);
	 	 		SendClientMessage(playerid,COLOR_YELLOW,string3);
				format(string3,sizeof(string3),"Deine Fraktion erhält noch %d Waffenteile und %d$",ak1,ak2);
	 	 		SendClientMessage(playerid,COLOR_YELLOW,string3);
			}
			else if(SpielerInfo[playerid][sFraktion] == 1)
			{
				format(string3, sizeof(string3), "HQ: Officer %s hat die Beute der Asservatenkammer in Los Santos erfolgreich beschlagnahmt.",sendername);
				SendCopMessage(COLOR_BLUE,string3);
				{
				   new string1[1000];
				   format(string1, sizeof(string1), "ACHTUNG: Officer %s hat die Asservatenkammer Los Santos erfolgreich beschlagnahmt!",sendername);
	               SendClientMessageToAll(COLOR_YELLOW, string1);
				}
				LSAsservatenkammerPickupAllow = 0;
				new bakmoney = 5000+ random(7500);
    			GivePlayerMoneyEx(playerid,bakmoney);
				format(string3,sizeof(string3),"Durch das beschlagnahmen der Asservatenkammer Los Santos, erhälst du %d$.",bakmoney);
	 	 		SendClientMessage(playerid,COLOR_YELLOW,string3);
			}
			foreach(new i : Player)
            {
                if(SpielerInfo[i][sFraktion] == fID)
                {
                    SendClientMessage(i,COLOR_SUPER,string3);
                }
            }
			return 1;
		}
	}
    return 1;
}

stock Evidence_EnterExit(playerid) {
    if(IsPlayerInRangeOfPoint(playerid, 2, 1797.6967,-1578.9346,14.0867))//asservatenkammer
    {
        SetTimerEx("Unfreeze",3000,false,"i",playerid);
        TogglePlayerControllable(playerid, false);
        SetPlayerPos(playerid,1821.1593,-1959.9767,1595.9619);
        SetPlayerFacingAngle(playerid, 268.7846);
        SetPlayerInterior(playerid,3);
        SetCameraBehindPlayer(playerid);
        return 1;
    }
    else if(IsPlayerInRangeOfPoint(playerid, 2, 1821.1593,-1959.9767,1595.9619))//asservatenkammer
    {
        TogglePlayerControllable(playerid, false);
        SetPlayerPos(playerid,1797.6967,-1578.9346,14.0867);
        SetPlayerFacingAngle(playerid, 268.7846);
        SetPlayerInterior(playerid,0);
        SetCameraBehindPlayer(playerid);
        return 1;
    }
    return 0;
}